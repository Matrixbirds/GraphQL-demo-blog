'use strict';
module.exports = function <%= type_name %>({
    GraphQLObjectType,
    GraphQLSchema,
    GraphQLString,
    GraphQLList,
    GraphQLNonNull,
    GraphQLInt,
}) {
    const {<%= type_name %>} = require('../../models');
    const <%= type_name %> = new GraphQLObjectType({
        name: '<%= `${type_name}` %>' ,
        description: '...',

        fields: () => ({<% for (let [field, type] of Object.entries(field_tables)) { %>
            <%= field %>: {
                type: <%= type %>,
            },
        <% } %>})
    });
    return {
        itemType: <%= type_name %>Type,
        model: <%= type_name %>
    };
}
