'use strict';
module.exports = ({
    GraphQLObjectType,
    GraphQLSchema,
    GraphQLString,
    GraphQLList,
    GraphQLNonNull,
    GraphQLInt,
}) => {
    const {<%= name %>} = require('../../models');
    const <%= type_name %> = new GraphQLObjectType({
        name: '<%= `${name}` %>' ,
        description: '...',

        fields: () => ({<% for (let [field, type] of fields_table) { %>
            <%= field %>: {
                type: <%= type %>,
            },<% } %>
        })
    });
    return {
        itemType: <%= type_name %>,
        model: <%= name %>
    };
}
